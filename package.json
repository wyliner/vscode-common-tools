{
  "name": "common-tools",
  "displayName": "Common Tools",
  "description": "Common Tools for VS Code",
  "keywords": [
    "vscode",
    "tools",
    "string",
    "json",
    "time",
    "uuid",
    "base64"
  ],
  "icon": "icon/icon.png",
  "version": "0.0.11",
  "publisher": "YonglinWang",
  "engines": {
    "vscode": "^1.101.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wyliner/vscode-common-tools"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
